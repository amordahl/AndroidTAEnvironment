<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<config>
    <androidPlatforms>/home/vagrant/platforms/</androidPlatforms>
    <maxMemory>4</maxMemory>
    <tools>
        <tool name="DidFail" version="1">
            <priority>1</priority>
            <path>/home/vagrant/AndroidTAEnvironment/tools/DidFail/AQL</path>
            <run>/home/vagrant/AndroidTAEnvironment/tools/DidFail/cert/run-didfail.sh /home/vagrant/AndroidTAEnvironment/tools/DidFail/AQL/results/%APP_APK_FILENAME% %APP_APK%</run>
            <runOnExit>/home/vagrant/AndroidTAEnvironment/AQL-System/flushMemory.sh</runOnExit>
            <runOnAbort>/home/vagrant/AndroidTAEnvironment/AQL-System/killpid.sh %PID%</runOnAbort>
            <result>/home/vagrant/AndroidTAEnvironment/tools/DidFail/AQL/results/%APP_APK_FILENAME%</result>
            <questions>InterAppFlows, IntraAppFlows</questions>
            <instances>0</instances>
            <memoryPerInstance>4</memoryPerInstance>
        </tool>
    </tools>
</config>
